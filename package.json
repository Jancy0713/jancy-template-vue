{
  "name": "jancy-template-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ai-debug": "cross-env DEBUG=vitest:* vitest run --config=vitest.ai-debug.config.ts",
    "test:unit:ai-debug:ui": "cross-env DEBUG=vitest:* vitest --ui --config=vitest.ai-debug.config.ts",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:safari": "playwright test --project=webkit",
    "test:e2e:ai-debug": "cross-env PWDEBUG=console DEBUG=pw:api playwright test --config=playwright.ai-debug.config.ts",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "swagger": "sh ./scripts/openapi-generator.sh"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@tailwindcss/vite": "^4.1.10",
    "@types/tinycolor2": "^1.4.6",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "element-plus": "^2.10.2",
    "pinia": "^3.0.1",
    "rxjs": "^7.8.2",
    "tailwindcss": "^4.1.10",
    "tinycolor2": "^1.6.0",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-i18n": "^12.0.0-alpha.2",
    "vue-router": "^4.5.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.19",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "lodash": "^4.17.21",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  }
}
